{
  "author": "",
  "description": "WOPR TTS plugin for Chatterbox (OpenAI-compatible)",
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "^25.1.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "wopr",
    "plugin",
    "tts",
    "chatterbox",
    "voice",
    "speech"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "wopr-plugin-voice-chatterbox",
  "scripts": {
    "build": "tsc",
    "check": "biome check src/ && tsc --noEmit",
    "clean": "rm -rf dist",
    "format": "biome format --write src/",
    "lint": "biome check src/",
    "lint:fix": "biome check --fix src/"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "version": "1.0.0"
}
